CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;

    PARTS:
    DMux4Way(in= in, sel= sel[1..2], a= t1, b= t2, c= t3, d= t4);
    DMux(in= t1, sel= sel[0], a= a, b= b);
    DMux(in= t2, sel= sel[0], a= c, b= d);
    DMux(in= t3, sel= sel[0], a= e, b= f);
    DMux(in= t4, sel= sel[0], a= g, b= h);
}